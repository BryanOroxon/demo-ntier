@model IEnumerable<Music.Entities.Album>
@{
    ViewData["Title"] = "Index";
}

<h2>Albumes</h2>


<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                   
                </td>
            </tr>
        }
    </tbody>
</table>



